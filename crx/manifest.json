{
    "name": "Rumor Recognize Extensions",
    "description" : "Rumor Recognize Extensions",
    "version": "1.0",
    "manifest_version": 2,

    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon/shandian.png"
    },
     // 快捷键打开
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens popup.html"
      }
    }
}